# === Backend ===
SECRET_KEY=change_me_to_a_long_random_string
JWT_ALGORITHM=HS256
JWT_EXPIRES_IN=86400

ADMIN_USERNAME=change_me
ADMIN_PASSWORD=change_me

MONGODB_URI=mongodb+srv://<user>:<pass>@cluster0.xxx.mongodb.net/gmb?retryWrites=true&w=majority
ALLOWED_ORIGINS=https://yourdomain.com,https://yourpreview.vercel.app

# Optional
PORT=8000
LOG_LEVEL=info

# === Test Runner ===
GMB_BASE_URL=https://your-backend-domain/api
GMB_ADMIN_USERNAME=${ADMIN_USERNAME}
GMB_ADMIN_PASSWORD=${ADMIN_PASSWORD}
GMB_HTTP_TIMEOUT=15
GMB_HTTP_RETRIES=3
